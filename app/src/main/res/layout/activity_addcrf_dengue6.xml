<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/theme_cream"
    tools:context=".AddcrfDengueActivity">

    <LinearLayout
        android:id="@+id/dengueBars6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <View
            android:layout_width="35dp"
            android:layout_height="5.1dp"
            android:layout_marginLeft="35.6dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="35dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="35dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="35dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="35dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="35dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="35dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_blue"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="35dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35.6dp"
        android:orientation="vertical"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/dengueBars6"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:text="Laboratory Tests"
            android:textColor="@color/theme_green"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/dengue_header6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/worksans_semibold"
            android:text="Add a Dengue Case"
            android:textColor="@color/theme_blue"
            android:textSize="30dp" />
    </LinearLayout>

    <ScrollView
        android:id="@+id/sv_den_6"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="150dp"
        android:layout_marginBottom="85dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:scrollbars="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/dengueBars6"
        app:layout_constraintVertical_bias="1.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cons_den6a"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="50dp">
            <EditText
                android:id="@+id/et_den_labStatus"
                style="@style/Input"
                android:layout_width="320dp"
                android:layout_height="1.5dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/inputbox"
                android:inputType="text"
                android:textColor="@color/black"
                android:textSize="18dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/cons_den6a"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="-10dp"
                android:background="@color/theme_cream"
                android:text="   Do you have the lab result? *  "
                android:textColor="@color/light_gray"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="@id/et_den_labStatus"
                app:layout_constraintTop_toTopOf="@id/et_den_labStatus" />

            <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/radiogroup_den_labResult"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_den_labStatus"
                tools:ignore="MissingConstraints">
                <RadioButton
                    android:id="@+id/radiobutton_den_labResultNo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="No"
                    android:textColor="@color/theme_darkgray" />
                <RadioButton
                    android:id="@+id/radiobutton_den_labResultProcessing"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="Processing"
                    android:textColor="@color/theme_darkgray" />
                <RadioButton
                    android:id="@+id/radiobutton_den_labResultYes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="Yes"
                    android:textColor="@color/theme_darkgray" />
            </RadioGroup>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/radiogroup_den_labResult">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:visibility="gone"
                    android:id="@+id/cons_den_noLabResult"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintTop_toBottomOf="@id/radiogroup_den_labResult"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/tv_den_labselect"
                        style="@style/Input"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:background="@drawable/inputbox"
                        android:inputType="text"
                        android:layout_marginTop="10dp"
                        android:paddingTop="15dp"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="@id/cons_den_noLabResult" />
                    <Spinner
                        android:id="@+id/spinner_den_labselect"
                        style="@style/Widget.AppCompat.Spinner.DropDown"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:layout_marginTop="10dp"
                        android:spinnerMode="dropdown"
                        android:textColor="#FFFBEE"
                        android:theme="@style/ThemeOverlay.AppCompat.Light"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="@id/tv_den_labselect" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="   Select Lab *  "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/tv_den_labselect"
                        app:layout_constraintTop_toTopOf="@id/tv_den_labselect" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/radiogroup_den_labResult">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:visibility="gone"
                    android:id="@+id/cons_den_yesLabResult"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintTop_toBottomOf="@id/radiogroup_den_labResult"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/tv_ns1_header"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="NS1"
                        android:textColor="@color/theme_blue"
                        android:fontFamily="@font/worksans_semibold"
                        android:textSize="20dp"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginTop="10dp"
                        tools:ignore="MissingConstraints,NotSibling" />

                    <EditText
                        android:id="@+id/et_den_ns1"
                        style="@style/Input"
                        android:layout_width="320dp"
                        android:layout_height="47dp"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/inputbox"
                        android:inputType="textCapCharacters"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_ns1_header" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="   Date Done   "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_den_ns1"
                        app:layout_constraintTop_toTopOf="@id/et_den_ns1" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cons_den_ns1_result"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="8dp"
                        app:layout_constraintTop_toBottomOf="@id/et_den_ns1"
                        tools:ignore="MissingConstraints">

                        <TextView
                            android:id="@+id/tv_den_ns1_result"
                            style="@style/Input"
                            android:layout_width="match_parent"
                            android:layout_height="47dp"
                            android:background="@drawable/inputbox"
                            android:inputType="text"
                            android:layout_marginTop="10dp"
                            android:paddingTop="15dp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="18dp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="@id/cons_den_ns1_result" />
                        <Spinner
                            android:id="@+id/spinner_den_ns1select"
                            style="@style/Widget.AppCompat.Spinner.DropDown"
                            android:layout_width="match_parent"
                            android:layout_height="47dp"
                            android:layout_marginTop="10dp"
                            android:spinnerMode="dropdown"
                            android:textColor="#FFFBEE"
                            android:theme="@style/ThemeOverlay.AppCompat.Light"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="@id/tv_den_ns1_result" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="-10dp"
                            android:background="@color/theme_cream"
                            android:text="   Result  "
                            android:textColor="@color/light_gray"
                            android:textSize="15dp"
                            app:layout_constraintLeft_toLeftOf="@id/tv_den_ns1_result"
                            app:layout_constraintTop_toTopOf="@id/tv_den_ns1_result" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/tv_igg_header"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="IgG ELISA"
                        android:textColor="@color/theme_blue"
                        android:fontFamily="@font/worksans_semibold"
                        android:textSize="20dp"
                        app:layout_constraintTop_toBottomOf="@id/cons_den_ns1_result"
                        android:layout_marginTop="10dp"
                        tools:ignore="MissingConstraints,NotSibling" />

                    <EditText
                        android:id="@+id/et_den_igg"
                        style="@style/Input"
                        android:layout_width="320dp"
                        android:layout_height="47dp"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/inputbox"
                        android:inputType="textCapCharacters"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_igg_header" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="   Date Done   "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_den_igg"
                        app:layout_constraintTop_toTopOf="@id/et_den_igg" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cons_den_igg_result"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="8dp"
                        app:layout_constraintTop_toBottomOf="@id/et_den_igg"
                        tools:ignore="MissingConstraints">

                        <TextView
                            android:id="@+id/tv_den_igg_result"
                            style="@style/Input"
                            android:layout_width="match_parent"
                            android:layout_height="47dp"
                            android:background="@drawable/inputbox"
                            android:inputType="text"
                            android:layout_marginTop="10dp"
                            android:paddingTop="15dp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="18dp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="@id/cons_den_igg_result" />
                        <Spinner
                            android:id="@+id/spinner_den_iggselect"
                            style="@style/Widget.AppCompat.Spinner.DropDown"
                            android:layout_width="match_parent"
                            android:layout_height="47dp"
                            android:layout_marginTop="10dp"
                            android:spinnerMode="dropdown"
                            android:textColor="#FFFBEE"
                            android:theme="@style/ThemeOverlay.AppCompat.Light"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="@id/tv_den_igg_result" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="-10dp"
                            android:background="@color/theme_cream"
                            android:text="   Result  "
                            android:textColor="@color/light_gray"
                            android:textSize="15dp"
                            app:layout_constraintLeft_toLeftOf="@id/tv_den_igg_result"
                            app:layout_constraintTop_toTopOf="@id/tv_den_igg_result" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/tv_igm_header"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="IgM ELISA"
                        android:textColor="@color/theme_blue"
                        android:fontFamily="@font/worksans_semibold"
                        android:textSize="20dp"
                        app:layout_constraintTop_toBottomOf="@id/cons_den_igg_result"
                        android:layout_marginTop="10dp"
                        tools:ignore="MissingConstraints,NotSibling" />

                    <EditText
                        android:id="@+id/et_den_igm"
                        style="@style/Input"
                        android:layout_width="320dp"
                        android:layout_height="47dp"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/inputbox"
                        android:inputType="textCapCharacters"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_igm_header" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="   Date Done   "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_den_igm"
                        app:layout_constraintTop_toTopOf="@id/et_den_igm" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cons_den_igm_result"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="8dp"
                        app:layout_constraintTop_toBottomOf="@id/et_den_igm"
                        tools:ignore="MissingConstraints">

                        <TextView
                            android:id="@+id/tv_den_igm_result"
                            style="@style/Input"
                            android:layout_width="match_parent"
                            android:layout_height="47dp"
                            android:background="@drawable/inputbox"
                            android:inputType="text"
                            android:layout_marginTop="10dp"
                            android:paddingTop="15dp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="18dp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="@id/cons_den_igm_result" />
                        <Spinner
                            android:id="@+id/spinner_den_igmselect"
                            style="@style/Widget.AppCompat.Spinner.DropDown"
                            android:layout_width="match_parent"
                            android:layout_height="47dp"
                            android:layout_marginTop="10dp"
                            android:spinnerMode="dropdown"
                            android:textColor="#FFFBEE"
                            android:theme="@style/ThemeOverlay.AppCompat.Light"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="@id/tv_den_igm_result" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="-10dp"
                            android:background="@color/theme_cream"
                            android:text="   Result  "
                            android:textColor="@color/light_gray"
                            android:textSize="15dp"
                            app:layout_constraintLeft_toLeftOf="@id/tv_den_igm_result"
                            app:layout_constraintTop_toTopOf="@id/tv_den_igm_result" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/tv_pcr_header"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="PCR"
                        android:textColor="@color/theme_blue"
                        android:fontFamily="@font/worksans_semibold"
                        android:textSize="20dp"
                        app:layout_constraintTop_toBottomOf="@id/cons_den_igm_result"
                        android:layout_marginTop="10dp"
                        tools:ignore="MissingConstraints,NotSibling" />

                    <EditText
                        android:id="@+id/et_den_pcr"
                        style="@style/Input"
                        android:layout_width="320dp"
                        android:layout_height="47dp"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/inputbox"
                        android:inputType="textCapCharacters"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_pcr_header" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="   Date Done   "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_den_pcr"
                        app:layout_constraintTop_toTopOf="@id/et_den_pcr" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cons_den_pcr_result"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="8dp"
                        app:layout_constraintTop_toBottomOf="@id/et_den_pcr"
                        tools:ignore="MissingConstraints">

                        <TextView
                            android:id="@+id/tv_den_pcr_result"
                            style="@style/Input"
                            android:layout_width="match_parent"
                            android:layout_height="47dp"
                            android:background="@drawable/inputbox"
                            android:inputType="text"
                            android:layout_marginTop="10dp"
                            android:paddingTop="15dp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="18dp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="@id/cons_den_pcr_result" />
                        <Spinner
                            android:id="@+id/spinner_den_pcrselect"
                            style="@style/Widget.AppCompat.Spinner.DropDown"
                            android:layout_width="match_parent"
                            android:layout_height="47dp"
                            android:layout_marginTop="10dp"
                            android:spinnerMode="dropdown"
                            android:textColor="#FFFBEE"
                            android:theme="@style/ThemeOverlay.AppCompat.Light"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
