<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/theme_cream"
    tools:context=".AddcifMeaslesActivity">

    <LinearLayout
        android:id="@+id/measlesBars5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginLeft="35.6dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_blue"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35.6dp"
        android:orientation="vertical"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/measlesBars5"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:text="Exposure History"
            android:textColor="@color/theme_green"
            android:textSize="13dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:fontFamily="@font/worksans_semibold"
            android:text="Add a Measles Case"
            android:textColor="@color/theme_blue"
            android:textSize="26dp" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="125dp"
        android:layout_marginBottom="85dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:scrollbars="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/measlesBars5"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cons_mea5a"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="50dp">

            <EditText
                android:id="@+id/et_mea_travelhistory"
                style="@style/Input"
                android:layout_width="320dp"
                android:layout_height="1.5dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/inputbox"
                android:inputType="text"
                android:textColor="@color/black"
                android:textSize="18dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/cons_mea5a" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="-10dp"
                android:background="@color/theme_cream"
                android:text="   History of travel 23 days prior to rash *  "
                android:textColor="@color/light_gray"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="@id/et_mea_travelhistory"
                app:layout_constraintTop_toTopOf="@id/et_mea_travelhistory" />
            <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/radiogroup_mea_travelhistory"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_mea_travelhistory"
                tools:ignore="MissingConstraints">
                <RadioButton
                    android:id="@+id/radiobutton_mea_travelhistoryNo"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="No"
                    android:textColor="@color/theme_darkgray" />
                <RadioButton
                    android:id="@+id/radiobutton_mea_travelhistoryYes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="Yes"
                    android:textColor="@color/theme_darkgray" />
            </RadioGroup>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cons_mea5b"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:layout_marginBottom="15dp"
                android:paddingLeft="20dp"
                android:paddingRight="15dp"
                app:layout_constraintTop_toBottomOf="@id/et_mea_travelhistory"
                tools:ignore="MissingConstraints">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cons_mea_withtravelhistory"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@id/cons_mea5b"
                    android:layout_marginTop="5dp"
                    android:paddingBottom="10dp"
                    tools:ignore="MissingConstraints">

                    <EditText
                        android:id="@+id/et_mea_travelplace"
                        style="@style/Input"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginLeft="8dp"
                        android:background="@drawable/inputbox"
                        android:focusable="false"
                        android:inputType="date"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintTop_toTopOf="@id/cons_mea_withtravelhistory"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="  Travel Place *  "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_mea_travelplace"
                        app:layout_constraintTop_toTopOf="@id/et_mea_travelplace" />

                    <EditText
                        android:id="@+id/et_mea_traveldate"
                        style="@style/Input"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:layout_marginTop="65dp"
                        android:layout_marginLeft="8dp"
                        android:background="@drawable/inputbox"
                        android:focusable="false"
                        android:inputType="date"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintTop_toTopOf="@id/et_mea_travelplace"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="  Travel Date *  "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_mea_traveldate"
                        app:layout_constraintTop_toTopOf="@id/et_mea_traveldate" />

                    <EditText
                        android:id="@+id/et_mea_travelrashonset"
                        style="@style/Input"
                        android:layout_width="match_parent"
                        android:layout_height="1.5dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginLeft="8dp"
                        android:background="@drawable/inputbox"
                        android:focusable="false"
                        android:inputType="text"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintTop_toBottomOf="@id/et_mea_traveldate"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="  Date from rash onset *  "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_mea_travelrashonset"
                        app:layout_constraintTop_toTopOf="@id/et_mea_travelrashonset" />
                    <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                        android:id="@+id/radiogroup_rashonsetdate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="10dp"
                        android:orientation="horizontal"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="@id/et_mea_travelrashonset"
                        tools:ignore="MissingConstraints">
                        <RadioButton
                            android:id="@+id/radiobutton_rashonsetdateLess"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="20dp"
                            android:buttonTint="@color/theme_darkgray"
                            android:text="Less than 7 days"
                            android:textColor="@color/theme_darkgray" />
                        <RadioButton
                            android:id="@+id/radiobutton_rashonsetdateMore"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="0dp"
                            android:buttonTint="@color/theme_darkgray"
                            android:text="7 - 23 days"
                            android:textColor="@color/theme_darkgray" />
                    </RadioGroup>

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <EditText
                android:id="@+id/et_measlesContact"
                style="@style/Input"
                android:layout_width="320dp"
                android:layout_height="1.5dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/inputbox"
                android:inputType="text"
                android:textColor="@color/black"
                android:textSize="18dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cons_mea5b" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="-10dp"
                android:background="@color/theme_cream"
                android:text="   Contact with Confirmed Measles *  "
                android:textColor="@color/light_gray"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="@id/et_measlesContact"
                app:layout_constraintTop_toTopOf="@id/et_measlesContact" />
            <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/radiogroup_measlescontact"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_measlesContact"
                tools:ignore="MissingConstraints">
                <RadioButton
                    android:id="@+id/radiobutton_measlescontactNo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="No"
                    android:textColor="@color/theme_darkgray" />
                <RadioButton
                    android:id="@+id/radiobutton_measlescontactUnknown"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="Unknown"
                    android:textColor="@color/theme_darkgray" />
                <RadioButton
                    android:id="@+id/radiobutton_measlescontactYes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="Yes"
                    android:textColor="@color/theme_darkgray" />
            </RadioGroup>

            <EditText
                android:id="@+id/et_rubellaContact"
                style="@style/Input"
                android:layout_width="320dp"
                android:layout_height="1.5dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/inputbox"
                android:inputType="text"
                android:textColor="@color/black"
                android:textSize="18dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/radiogroup_measlescontact" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="-10dp"
                android:background="@color/theme_cream"
                android:text="   Contact with Confirmed Rubella *  "
                android:textColor="@color/light_gray"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="@id/et_rubellaContact"
                app:layout_constraintTop_toTopOf="@id/et_rubellaContact" />
            <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/radiogroup_rubellacontact"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_rubellaContact"
                tools:ignore="MissingConstraints">
                <RadioButton
                    android:id="@+id/radiobutton_rubellacontactNo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="No"
                    android:textColor="@color/theme_darkgray" />
                <RadioButton
                    android:id="@+id/radiobutton_rubellacontactUnknown"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="Unknown"
                    android:textColor="@color/theme_darkgray" />
                <RadioButton
                    android:id="@+id/radiobutton_rubellacontactYes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="Yes"
                    android:textColor="@color/theme_darkgray" />
            </RadioGroup>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cons_mea5c"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/et_rubellaContact"
                android:layout_marginTop="50dp">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:visibility="gone"
                    android:id="@+id/cons_mea_confirmedrubella"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="20dp"
                    android:paddingRight="15dp"
                    tools:ignore="MissingConstraints">
                    <EditText
                        android:id="@+id/et_rubella_contactname"
                        style="@style/Input"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginLeft="8dp"
                        android:background="@drawable/inputbox"
                        android:focusable="false"
                        android:inputType="textCapCharacters"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintTop_toTopOf="@id/cons_mea_confirmedrubella"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="  Contact Name *  "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_rubella_contactname"
                        app:layout_constraintTop_toTopOf="@id/et_rubella_contactname" />

                    <EditText
                        android:id="@+id/et_rubella_contactplace"
                        style="@style/Input"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:layout_marginTop="65dp"
                        android:layout_marginLeft="8dp"
                        android:background="@drawable/inputbox"
                        android:focusable="false"
                        android:inputType="textCapCharacters"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintTop_toTopOf="@id/et_rubella_contactname"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="  Residence/Place of Contact *  "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_rubella_contactplace"
                        app:layout_constraintTop_toTopOf="@id/et_rubella_contactplace" />

                    <EditText
                        android:id="@+id/et_rubella_traveldate"
                        style="@style/Input"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:layout_marginTop="65dp"
                        android:layout_marginLeft="8dp"
                        android:background="@drawable/inputbox"
                        android:focusable="false"
                        android:inputType="textCapCharacters"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintTop_toTopOf="@id/et_rubella_contactplace"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="  Travel date *  "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_rubella_traveldate"
                        app:layout_constraintTop_toTopOf="@id/et_rubella_traveldate" />

                    <EditText
                        android:id="@+id/et_rubella_exposure"
                        style="@style/Input"
                        android:layout_width="match_parent"
                        android:layout_height="1.5dp"
                        android:layout_marginTop="65dp"
                        android:layout_marginLeft="8dp"
                        android:background="@drawable/inputbox"
                        android:focusable="false"
                        android:inputType="text"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintTop_toTopOf="@id/et_rubella_traveldate"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="  Probable exposure occurance *  "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_rubella_exposure"
                        app:layout_constraintTop_toTopOf="@id/et_rubella_exposure" />
                    <RadioGroup
                        android:id="@+id/radiogroup_rubellaExposure"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="10dp"
                        android:orientation="vertical"
