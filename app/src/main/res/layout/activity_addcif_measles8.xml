<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/theme_cream"
    tools:context=".AddcifMeaslesActivity">

    <LinearLayout
        android:id="@+id/measlesBars8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginLeft="35.6dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_green"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:backgroundTint="@color/theme_blue"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <View
            android:layout_width="28dp"
            android:layout_height="5.1dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/bar"
            android:outlineAmbientShadowColor="@color/black"
            android:outlineSpotShadowColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35.6dp"
        android:orientation="vertical"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/measlesBars8"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:text="Laboratory Tests"
            android:textColor="@color/theme_green"
            android:textSize="13dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:fontFamily="@font/worksans_semibold"
            android:text="Add a Measles Case (CIF)"
            android:textColor="@color/theme_blue"
            android:textSize="26dp" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="125dp"
        android:layout_marginBottom="85dp"
        android:paddingLeft="36dp"
        android:paddingRight="36dp"
        android:scrollbars="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/measlesBars8"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cons_mea8a"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="50dp">
            <EditText
                android:id="@+id/et_mea_labStatus"
                style="@style/Input"
                android:layout_width="320dp"
                android:layout_height="1.5dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/inputbox"
                android:inputType="text"
                android:textColor="@color/black"
                android:textSize="18dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/cons_mea8a"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="-10dp"
                android:background="@color/theme_cream"
                android:text="   Do you have the lab result? *  "
                android:textColor="@color/light_gray"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="@id/et_mea_labStatus"
                app:layout_constraintTop_toTopOf="@id/et_mea_labStatus" />
            <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/radiogroup_mea_labresult"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_mea_labStatus"
                tools:ignore="MissingConstraints">
                <RadioButton
                    android:id="@+id/radiobutton_mea_labResultNo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="No"
                    android:textColor="@color/theme_darkgray" />
                <RadioButton
                    android:id="@+id/radiobutton_mea_labResultProcessing"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="Processing"
                    android:textColor="@color/theme_darkgray" />
                <RadioButton
                    android:id="@+id/radiobutton_mea_labResultYes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:buttonTint="@color/theme_darkgray"
                    android:text="Yes"
                    android:textColor="@color/theme_darkgray" />
            </RadioGroup>

            <LinearLayout
                android:id="@+id/cons_mea_8a"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:paddingLeft="20dp"
                android:paddingRight="15dp"
                app:layout_constraintTop_toTopOf="@id/radiogroup_mea_labresult"
                tools:ignore="MissingConstraints"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:visibility="gone"
                    android:id="@+id/cons_mea_withlabresult"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintTop_toBottomOf="@id/et_mea_travelhistory"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/tv_mea_labspecimen"
                        style="@style/Input"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:background="@drawable/inputbox"
                        android:inputType="text"
                        android:layout_marginTop="10dp"
                        android:paddingTop="15dp"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="@id/cons_mea_withlabresult" />
                    <Spinner
                        android:id="@+id/spinner_mea_labspecimen"
                        style="@style/Widget.AppCompat.Spinner.DropDown"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:layout_marginTop="10dp"
                        android:spinnerMode="dropdown"
                        android:textColor="#FFFBEE"
                        android:theme="@style/ThemeOverlay.AppCompat.Light"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="@id/tv_mea_labspecimen" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="   Lab Specimen *  "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/tv_mea_labspecimen"
                        app:layout_constraintTop_toTopOf="@id/tv_mea_labspecimen" />

                    <LinearLayout
                        android:id="@+id/cons_mea8b"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_mea_labspecimen"
                        tools:ignore="MissingConstraints">
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/cons_mea8c"
                            android:layout_width="145dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="6dp"
                            app:layout_constraintLeft_toLeftOf="parent">
                            <EditText
                                android:id="@+id/et_mea_collectdate"
                                style="@style/Input"
                                android:layout_width="match_parent"
                                android:layout_height="47dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/inputbox"
                                android:focusable="false"
                                android:inputType="date"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="18dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintTop_toTopOf="@id/cons_mea8c" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="-10dp"
                                android:background="@color/theme_cream"
                                android:text="  Date collected *  "
                                android:textColor="@color/light_gray"
                                android:textSize="14dp"
                                app:layout_constraintLeft_toLeftOf="@id/et_mea_collectdate"
                                app:layout_constraintTop_toTopOf="@id/et_mea_collectdate" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/cons_mea8d"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <EditText
                                android:id="@+id/et_mea_receivedate"
                                style="@style/Input"
                                android:layout_width="match_parent"
                                android:layout_height="47dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/inputbox"
                                android:focusable="false"
                                android:inputType="date"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="18dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintTop_toTopOf="@id/cons_mea8d" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="-10dp"
                                android:background="@color/theme_cream"
                                android:text=" RITM receive date *  "
                                android:textColor="@color/light_gray"
                                android:textSize="14dp"
                                app:layout_constraintLeft_toLeftOf="@id/et_mea_receivedate"
                                app:layout_constraintTop_toTopOf="@id/et_mea_receivedate" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/cons_mea8e"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/cons_mea8b"
                        tools:ignore="MissingConstraints">
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/cons_mea8f"
                            android:layout_width="145dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="6dp"
                            app:layout_constraintLeft_toLeftOf="parent">
                            <EditText
                                android:id="@+id/et_meaaslesresultIgm"
                                style="@style/Input"
                                android:layout_width="match_parent"
                                android:layout_height="47dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/inputbox"
                                android:focusable="false"
                                android:inputType="text"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="18dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintTop_toTopOf="@id/cons_mea8f" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="-10dp"
                                android:background="@color/theme_cream"
                                android:text="  Measles IgM *  "
                                android:textColor="@color/light_gray"
                                android:textSize="14dp"
                                app:layout_constraintLeft_toLeftOf="@id/et_meaaslesresultIgm"
                                app:layout_constraintTop_toTopOf="@id/et_meaaslesresultIgm" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/cons_mea8g"
                            android:layout_width="145dp"
                            android:layout_height="wrap_content">
                            <EditText
                                android:id="@+id/et_rubellaresultIgm"
                                style="@style/Input"
                                android:layout_width="match_parent"
                                android:layout_height="47dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/inputbox"
                                android:focusable="false"
                                android:inputType="text"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="18dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintTop_toTopOf="@id/cons_mea8g" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="-10dp"
                                android:background="@color/theme_cream"
                                android:text="  Rubella IgM *  "
                                android:textColor="@color/light_gray"
                                android:textSize="14dp"
                                app:layout_constraintLeft_toLeftOf="@id/et_rubellaresultIgm"
                                app:layout_constraintTop_toTopOf="@id/et_rubellaresultIgm" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/cons_mea8h"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/cons_mea8e"
                        tools:ignore="MissingConstraints">
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/cons_mea8i"
                            android:layout_width="145dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="6dp"
                            app:layout_constraintLeft_toLeftOf="parent">
                            <EditText
                                android:id="@+id/et_meaaslesresultVirusIsolation"
                                style="@style/Input"
                                android:layout_width="match_parent"
                                android:layout_height="47dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/inputbox"
                                android:focusable="false"
                                android:inputType="text"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="18dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintTop_toTopOf="@id/cons_mea8i" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="-10dp"
                                android:background="@color/theme_cream"
                                android:text="  Virus Isolation *  "
                                android:textColor="@color/light_gray"
                                android:textSize="14dp"
                                app:layout_constraintLeft_toLeftOf="@id/et_meaaslesresultVirusIsolation"
                                app:layout_constraintTop_toTopOf="@id/et_meaaslesresultVirusIsolation" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/cons_mea8j"
                            android:layout_width="145dp"
                            android:layout_height="wrap_content">
                            <EditText
                                android:id="@+id/et_meaaslesresultPRC"
                                style="@style/Input"
                                android:layout_width="match_parent"
                                android:layout_height="47dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/inputbox"
                                android:focusable="false"
                                android:inputType="text"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="18dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintTop_toTopOf="@id/cons_mea8j" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="-10dp"
                                android:background="@color/theme_cream"
                                android:text="  PRC *  "
                                android:textColor="@color/light_gray"
                                android:textSize="14dp"
                                app:layout_constraintLeft_toLeftOf="@id/et_meaaslesresultPRC"
                                app:layout_constraintTop_toTopOf="@id/et_meaaslesresultPRC" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </LinearLayout>

                    <EditText
                        android:id="@+id/et_mea_resultInvestigator"
                        style="@style/Input"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:layout_marginTop="15dp"
                        android:background="@drawable/inputbox"
                        android:focusable="false"
                        android:inputType="date"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        app:layout_constraintTop_toBottomOf="@id/cons_mea8h"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="-10dp"
                        android:background="@color/theme_cream"
                        android:text="  Investigator *  "
                        android:textColor="@color/light_gray"
                        android:textSize="15dp"
                        app:layout_constraintLeft_toLeftOf="@id/et_mea_resultInvestigator"
                        app:layout_constraintTop_toTopOf="@id/et_mea_resultInvestigator" />

                    <LinearLayout
                        android:id="@+id/cons_mea8k"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/et_mea_resultInvestigator"
                        tools:ignore="MissingConstraints">
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/cons_mea8l"
                            android:layout_width="145dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="6dp"
                            app:layout_constraintLeft_toLeftOf="parent">
                            <EditText
                                android:id="@+id/et_mea_resultInvestigatorContact"
                                style="@style/Input"
                                android:layout_width="match_parent"
                                android:layout_height="47dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/inputbox"
                                android:focusable="false"
                                android:inputType="text"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="18dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintTop_toTopOf="@id/cons_mea8l" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="-10dp"
                                android:background="@color/theme_cream"
                                android:text="  Contact No *  "
                                android:textColor="@color/light_gray"
                                android:textSize="14dp"
                                app:layout_constraintLeft_toLeftOf="@id/et_mea_resultInvestigatorContact"
                                app:layout_constraintTop_toTopOf="@id/et_mea_resultInvestigatorContact" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/cons_mea8m"
                            android:layout_width="145dp"
                            android:layout_height="wrap_content">
                            <EditText
                                android:id="@+id/et_mea_resultInvestiDate"
                                style="@style/Input"
                                android:layout_width="match_parent"
                                android:layout_height="47dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/inputbox"
                                android:focusable="false"
                                android:inputType="text"
                                android:textAllCaps="true"
